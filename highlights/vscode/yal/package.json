{
    "name": "yal",
    "displayName": "YAL",
    "description": "YAL - Yet Another Language Programming Language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "semanticHighlighting": true,
    "contributes": {
